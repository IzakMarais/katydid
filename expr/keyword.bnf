  : "!" << &expr.Keyword{Value: "!"}, nil >>
  | Space "!" << 
    &expr.Keyword{
      Before: $0.(*expr.Space),
      Value: "!",
    }, nil
  >>
  ;

Star
  : "*" << &expr.Keyword{Value: "*"}, nil >>
  | Space "*" << 
    &expr.Keyword{
